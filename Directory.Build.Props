<Project>
  <PropertyGroup>
    <Authors>SharperHacks LLC (WA), Joseph W Donahue</Authors>
    <Copyright>$(Authors)</Copyright>
    <Company>Sharper Hacks LLC (US-WA)</Company>
    <CompanyNamespace>SharperHacks</CompanyNamespace>
    <CollectionName>CoreLibs</CollectionName>
  </PropertyGroup>

  <PropertyGroup>
    <SuppressNETCoreSdkPreviewMessage>true</SuppressNETCoreSdkPreviewMessage>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Deterministic>true</Deterministic>
    <PathMapRoot>$(CompanyNamespace)/$(CollectionName)</PathMapRoot>
  </PropertyGroup>

  <PropertyGroup>
    <ExtraTargetFramework Condition="'$(VisualStudioVersion)' >= '18.0'">net10.0</ExtraTargetFramework>
    <TargetFrameworks>$(ExtraTargetFramework);net9.0;net8.0;</TargetFrameworks>
  </PropertyGroup>
</Project>
